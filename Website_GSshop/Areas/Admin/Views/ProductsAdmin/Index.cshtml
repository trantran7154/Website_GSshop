<div ng-app="myModule">
    <div ng-controller="myControllerPro">
        <div class="card mt-3" style="padding: 10px">
            <div class="row">
                <div class="col-lg-3">
                    <label class="font-weight-bold">Tìm kiếm theo mã</label>
                    <div class="ui icon input" style="width: 100%">
                        <i class="search icon"></i>
                        <input type="text" class="form-control" placeholder="Nhập mã sản phẩm" ng-model="searchText.id">
                    </div>
                </div>
                <div class="col-lg-3">
                    <label class="font-weight-bold">Danh mục chính</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.cate">
                            <option value="">Tất cả</option>
                            @foreach (var item in category)
                            {
                                <option value="@item.category_name">@item.category_name</option>
                            }
                        </select>
                    </div>
                </div>
                <div class="col-lg-3">
                    <label class="font-weight-bold">Danh mục phụ</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.sub">
                            <option value="">Tất cả</option>
                            @foreach (var item in subCategories)
                            {
                                <option value="@item.subcategory_name">@item.subcategory_name</option>
                            }
                        </select>
                    </div>
                </div>
                <div class="col-lg-3">
                    <label class="font-weight-bold">Tình trạng</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.ac">
                            <option value="">Tất cả</option>
                            <option value="true">Đang hoạt động</option>
                            <option value="false">Không hoạt động</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3 mt-3">
                    <label class="font-weight-bold">Thùng rác</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.bin">
                            <option value="">Tất cả</option>
                            <option value="false">Đã xóa</option>
                            <option value="true">Chưa xóa</option>
                        </select>
                    </div>
                </div>
                <div class="col-lg-3 mt-3">
                    <label class="font-weight-bold">Email đăng</label>
                    <div class="ui icon input" style="width: 100%">
                        <i class="search icon"></i>
                        <input type="text" class="form-control" placeholder="Nhập email" ng-model="searchText.email">
                    </div>
                </div>
                <div class="col-lg-3 mt-3">
                    <label class="font-weight-bold">Giá</label>
                    <div class="ui icon input" style="width: 100%">
                        <i class="search icon"></i>
                        <input type="text" class="form-control" placeholder="Nhập giá" ng-model="searchText.price">
                    </div>
                </div>
                <div class="col-lg-3 mt-3">
                    <label class="font-weight-bold">Giá sale</label>
                    <div class="ui icon input" style="width: 100%">
                        <i class="search icon"></i>
                        <input type="text" class="form-control" placeholder="Nhập giá sale" ng-model="searchText.sale">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-2 mt-3">
                    <label class="font-weight-bold">Ngày</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.date">
                            <option value="">Tất cả</option>
                            @for (var i = 1; i <= 31; i++)
                            {
                                <option value="@i">Ngày @i</option>
                            }
                        </select>
                    </div>
                </div>
                <div class="col-lg-2 mt-3">
                    <label class="font-weight-bold">Tháng</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.month">
                            <option value="">Tất cả</option>
                            @for (var i = 1; i <= 12; i++)
                            {
                                <option value="@i">Tháng @i</option>
                            }
                        </select>
                    </div>
                </div>
                <div class="col-lg-2 mt-3">
                    <label class="font-weight-bold">Năm</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.year">
                            <option value="">Tất cả</option>
                            @for (var i = 2019; i <= Int32.Parse(date.ToString("yyyy").ToString()); i++)
                            {
                                <option value="@i">Năm @i</option>
                            }
                        </select>
                    </div>
                </div>
            </div>
        </div>
        <table class="table table-bordered table-hover mt-4">
            <tr class="table-info text-center">
                <th>
                    <span><i class="fa fa-pencil"></i> Tên sản phẩm</span>
                </th>
                <th>
                    <span><i class="fa fa-image"></i> Ảnh</span>
                </th>
                <th>
                    <span><i class="fa fa-calendar"></i> Ngày tạo</span>
                </th>
                <th>
                    <span><i class="fa fa-toggle-on"></i> Hoạt động</span>
                </th>
                <th>
                    <span><i class="fa fa-tags"></i> Giá</span>
                </th>
                <th>
                    <span><i class="fa fa-eye"></i> Lượt xem</span>
                </th>
                <th>
                    <span><i class="fa fa-heart-o"></i> Yêu thích</span>
                </th>
                <th>
                    <span><i class="fa fa-magic"></i> Tùy chọn</span>
                </th>
                <th>
                    <span><i class="fa fa-percent"></i> Giá sale</span>
                </th>
                <th>
                    <span><i class="fa fa-list-ol"></i> Số lượng</span>
                </th>
                <th>
                    <span><i class="fa fa-trash-o"></i> Thùng rác</span>
                </th>
                <th>
                    <span><i class="fa fa-cogs"></i> Công cụ</span>
                </th>
            </tr>
            <tr class="text-center" ng-repeat="todo in filteredTodos | filter: searchText">
                <td width="120">
                    {{todo.name}}
                </td>
                <td>
                    <img src="~/Content/Layout/images/{{todo.img}}" height="100" width="100" />
                </td>
                <td>
                    {{todo.date}}/{{todo.month}}/{{todo.year}}
                </td>
                <td>
                    {{todo.ac}}
                </td>
                <td width="110">
                    {{todo.price}}
                </td>
                <td>
                    {{todo.view}}
                </td>
                <td>
                    {{todo.like}}
                </td>
                <td>
                    {{todo.option}}
                </td>
                <td width="110">
                    {{todo.sale}}
                </td>
                <td>
                    {{todo.amount}}
                </td>
                <td>
                    {{todo.bin}}
