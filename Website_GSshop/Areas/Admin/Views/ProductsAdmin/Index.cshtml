@using Website_GSshop.Models;
@model List<Product>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";

    Data_GSShopEntities db = new Data_GSShopEntities();

    DateTime date = DateTime.Now;
}
@{
    List<Category> category = db.Category.ToList();
    List<SubCategory> subCategories = db.SubCategory.ToList();
    List<ClassificationSubCategory> classificationSubCategories = db.ClassificationSubCategory.ToList();
}

@Html.Action("CSS_Sematic","Home")
<script>
    var todos = angular.module('myModule', ['ui.bootstrap']);

    todos.controller('myControllerPro', function ($scope) {
        $scope.filteredTodos = []
            , $scope.currentPage = 1
            , $scope.numPerPage = 8
            , $scope.maxSize = 5;

        $scope.makeTodos = function () {
            $scope.todos = [
                @foreach (var item in Model)
                {
                    @: {id: @item.product_id,name: "@Html.Raw(item.product_name)",ac: "@item.product_active",email: "@item.Seller.seller_email", price: "@{float price = (float)item.product_price;} @price.ToString("#,##0") ₫", view: "@item.product_view", like: "@item.product_love", option: "@item.product_option", sale: "@{float sale = ((float)item.product_price * (1 - ((float)item.product_percent / 100)));}@sale.ToString("#,##0") ₫", amount: "@item.product_amount", bin: "@item.product_bin", img: "@item.product_image",cate: "@item.Category.category_name",sub: "@item.SubCategory.subcategory_name",date: "@item.product_datecreated.Value.Day",month: "@item.product_datecreated.Value.Month",year: "@item.product_datecreated.Value.Year" },
                }
            ];

        };
        $scope.makeTodos();

        $scope.$watch('currentPage + numPerPage', function () {
            var begin = (($scope.currentPage - 1) * $scope.numPerPage)
                , end = begin + $scope.numPerPage;

            $scope.filteredTodos = $scope.todos.slice(begin, end);
        });
    });
</script>
