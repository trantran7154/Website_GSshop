                                        <div class="field">
                                            <label>Chi tiết</label>
                                            <textarea id="editor1" name="seller_detail"></textarea>
                                        </div>
                                        <div class="field">
                                            <label>Mô tả</label>
                                            <textarea id="editor2" name="seller_description"></textarea>
                                        </div>
                                    </div>
