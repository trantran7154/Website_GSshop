                                        <div class="field">
                                            <label>Mật khẩu</label>
                                            <input type="password" name="seller_pass" />
                                        </div>
                                    </div>
                                    <div class="three fields">
                                        <div class="field">
                                            <label>Tên shop</label>
                                            <input type="text" name="seller_nameshop" />
                                        </div>
                                        <div class="field">
                                            <label>Token</label>
                                            <input type="text" name="seller_token" />
                                        </div>
                                        <div class="field">
                                            <label>Logo</label>
                                            <input name="fileUpload1" type="file">
                                        </div>
                                    </div>
