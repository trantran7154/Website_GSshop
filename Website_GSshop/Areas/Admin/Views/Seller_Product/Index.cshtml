@using Website_GSshop.Models
@model List<Seller>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";

    var sel_edit = "seledit";
    var sel_detail = "seldetail";
    var sel_delete = "seldelete";

    DateTime date = DateTime.Now;
}
<style>
    .error {
        display: grid;
        align-items: center;
        min-height: 10vh;
    }
</style>
@Html.Action("CSS_Sematic", "Home")
<script>
    var app = angular
        .module("myModule", [])
        .controller("myController", function ($scope) {

            var se = [
                @foreach (var item in Model)
                {
                    @: { id: @item.seller_id, email: "@Html.Raw(item.seller_email)", pass: "@Html.Raw(@item.seller_pass)", name: "@Html.Raw(item.seller_nameshop)", logo: "@item.seller_logo", token: "@Html.Raw(item.seller_token)", ac: "@item.seller_active", day: "@item.seller_datecreated.Value.Day", month: "@item.seller_datecreated.Value.Month", year: "@item.seller_datecreated.Value.Year", dayl: "@item.seller_datelogin.Value.Day", monthl: "@item.seller_datelogin.Value.Month", yearl: "@item.seller_datelogin.Value.Year", role: "@item.seller_role", bin: "@item.seller_bin", nice: "@Html.Raw(item.seller_nicename)", add: "@Html.Raw(item.seller_address)", detail: "@Html.Raw(item.seller_detail)", desc: "@Html.Raw(item.seller_description)", prov: "@Html.Raw(item.seller_provincecity)", dis: "@Html.Raw(item.seller_district)", tel: "@item.seller_telephone", li1: "@item.seller_link1", li2: "@item.seller_link2", li3: "@item.seller_link3", sli1: "@item.seller_link1", sli2: "@item.seller_link2", sli3: "@item.seller_link3", date: "@item.seller_datecreated.Value.Year-@(item.seller_datecreated.Value.Month < 10 ? "0" + item.seller_datecreated.Value.Month.ToString() : item.seller_datecreated.Value.Month.ToString())-@(item.seller_datecreated.Value.Day < 10 ? "0" + item.seller_datecreated.Value.Day.ToString() : item.seller_datecreated.Value.Day.ToString())", datel: "@item.seller_datelogin.Value.Year-@(item.seller_datelogin.Value.Month < 10 ? "0" + item.seller_datelogin.Value.Month.ToString() : item.seller_datelogin.Value.Month.ToString())-@(item.seller_datelogin.Value.Day < 10 ? "0" + item.seller_datelogin.Value.Day.ToString() : item.seller_datelogin.Value.Day.ToString())" },
                }
            ];

            $scope.se = se;
        });
</script>

