                                        <div class="field">
                                            <label>Ngày tạo</label>
                                            <input type="date" name="seller_datecreated" />
                                        </div>
                                        <div class="field">
                                            <label>Ngày đăng nhập</label>
                                            <input type="date" name="seller_datelogin" />
                                        </div>
                                    </div>
                                    <div class="three fields">
                                        <div class="field">
                                            <label>Nice name</label>
                                            <input type="text" name="seller_nicename" />
                                        </div>
                                        <div class="field">
                                            <label>Số điện thoại</label>
                                            <input type="text" name="seller_telephone" />
                                        </div>
                                        <div class="field">
                                            <label>Hoạt động</label>
                                            <select name="seller_active">
                                                <option>True</option>
                                                <option>False</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="three fields">
                                        <div class="field">
                                            <label>Địa chỉ</label>
                                            <input type="text" name="seller_address" />
                                        </div>
                                        <div class="field">
                                            <label>Quận/Huyện</label>
                                            <select name="seller_district">
                                                <option>Chọn Quận/Huyện</option>
                                            </select>
                                        </div>
                                        <div class="field">
                                            <label>Tỉnh/TP</label>
                                            <select name="seller_provincecity">
                                                <option>Chọn Tỉnh/Thành Phố</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="three fields">
                                        <div class="field">
                                            <label>Link 1</label>
                                            <input type="text" name="seller_link1">
