<a class="btn btn-warning float-right text-primary" data-toggle="modal" data-target="#sel_create"> THÊM MỚI</a>
<div ng-app="myModule">
    <div ng-controller="myController">
        <div class="card mt-3" style="padding: 10px">
            <div class="row">
                <div class="col-lg-3">
                    <label class="font-weight-bold">Tìm kiếm theo mã</label>
                    <div class="ui icon input" style="width: 100%">
                        <i class="search icon"></i>
                        <input type="text" class="form-control" placeholder="Nhập mã người bán" ng-model="searchText.id" />
                    </div>
                </div>
                <div class="col-lg-3">
                    <label class="font-weight-bold">Tìm kiếm tên</label>
                    <div class="ui icon input" style="width: 100%">
                        <i class="search icon"></i>
                        <input type="text" class="form-control" placeholder="Nhập tên người bán" ng-model="searchText.name" />
                    </div>
                </div>
                <div class="col-lg-3">
                    <label class="font-weight-bold">Tìm kiếm theo email</label>
                    <div class="ui icon input" style="width: 100%">
                        <i class="search icon"></i>
                        <input type="text" class="form-control" placeholder="Nhập email người bán" ng-model="searchText.email" />
                    </div>
                </div>
                <div class="col-lg-3">
                    <label class="font-weight-bold">Tình trạng</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.ac">
                            <option value="">Tất cả</option>
                            <option value="true">Đang hoạt động</option>
                            <option value="false">Không hoạt động</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3 mt-3">
                    <label class="font-weight-bold">Thùng rác</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.bin">
                            <option value="">Tất cả</option>
                            <option value="false">Đã xóa</option>
                            <option value="true">Chưa xóa</option>
                        </select>
                    </div>
                </div>
                <div class="col-lg-2 mt-3">
                    <label class="font-weight-bold">Ngày</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.date">
                            <option value="">Tất cả</option>
                            @for (var i = 1; i <= 31; i++)
                            {
                                <option value="@i">Ngày @i</option>
                            }
                        </select>
                    </div>
                </div>
                <div class="col-lg-2 mt-3">
                    <label class="font-weight-bold">Tháng</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.month">
                            <option value="">Tất cả</option>
                            @for (var i = 1; i <= 12; i++)
                            {
                                <option value="@i">Tháng @i</option>
                            }
                        </select>
                    </div>
                </div>
                <div class="col-lg-2 mt-3">
                    <label class="font-weight-bold">Năm</label>
                    <div class="ui form">
                        <select class="ui search dropdown" ng-model="searchText.year">
                            <option value="">Tất cả</option>
                            @for (var i = 2019; i <= Int32.Parse(date.ToString("yyyy").ToString()); i++)
                            {
                                <option value="@i">Năm @i</option>
                            }
                        </select>
                    </div>
                </div>
            </div>
        </div>
        <table class="table table-bordered table-hover mt-4">
            <tr class="table-info text-center">
                <th>
                    <span><i class="fa fa-envelope"></i> Email</span>
                </th>
                <th width="100">
                    <span><i class="fa fa-key"></i> Mật khẩu</span>
                </th>
                <th width="100">
                    <span><i class="fa fa-id-card"></i> Tên shop</span>
                </th>
                <th width="150">
                    <span><i class="fa fa-image"></i> Logo</span>
                </th>
                <th>
                    <span><i class="fa fa-asterisk"></i> Token</span>
                </th>
                <th>
                    <span><i class="fa fa-toggle-on"></i> Hoạt động</span>
                </th>
                <th>
                    <span><i class="fa fa-calendar"></i> Ngày tạo</span>
                </th>
                <th>
                    <span><i class="fa fa-calendar"></i> Ngày đăng nhập</span>
                </th>
                <th width="80">
                    <span><i class="fa fa-tag"></i> Vai trò</span>
                </th>
                <th>
                    <span><i class="fa fa-trash-o"></i> Thùng rác</span>
                </th>
                <th>
                    <span><i class="fa fa-cogs"></i> Công cụ</span>
                </th>
            </tr>
            <tr ng-repeat="se in se | filter: searchText">
                <td class="text-center">
                    {{se.email}}
                </td>
                <td class="text-center">
                    {{se.pass}}
                </td>
                <td class="text-center">
                    {{se.name}}
                </td>
                <td class="text-center">
                    <img src="~/Content/Layout/images/{{se.logo}}" height="100" width="100" />
                </td>
                <td class="text-center">
                    {{se.token}}
                </td>
                <td class="text-center">
                    {{se.ac}}
                </td>
                <td class="text-center">
                    {{se.day}}/{{se.month}}/{{se.year}}
                </td>
                <td class="text-center">
                    {{se.dayl}}/{{se.monthl}}/{{se.yearl}}
                </td>
                <td class="text-center">
                    {{se.role}}
                </td>
                <td class="text-center">
                    {{se.bin}}
                </td>
                <td width="150">
                <div class="text-center">
