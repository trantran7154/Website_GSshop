                                @using (Html.BeginForm("ProductCreate", "Seller_Product", FormMethod.Post, new { @class = "ui form segment", @enctype = "multipart/form-data" }))
                                {
                                    <div class="two fields">
                                        <div class="field">
                                            <label>Email</label>
                                            <input type="email" name="seller_email">
